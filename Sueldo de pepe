class Empleado{
	var property categoria
	var property neto = if(categoria == "cadete"){return 1500}else 1000
	var property inasistencias
	const bonopresentismo = if(inasistencias == 0){return 100}else(if(inasistencias == 1){return 50} else 0)
	const bonoresultados
	const property sueldo = neto + bonopresentismo + bonoresultados
}

const pepe = new Empleado(categoria = "cadete", inasistencias = 1, bonoresultados = 80)
